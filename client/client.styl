body
  padding 50px 0px
  background #222

left-width = 200px

.left
  width left-width
  float left
  text-align right
  padding 15px

.fixed-fluid
  margin-left left-width
  padding 15px
  border-left thin solid #ccc

.fixed-fixed
  margin-left left-width
  padding 15px
  border-left thin solid #ccc
  width 600px

.folder-entry
  border-radius 5px
  padding 5px
  margin 5px 0px

.fixed-thumb
  width 180px
  height 320px
  float left
  margin 3px

.thumbnail
  border none
